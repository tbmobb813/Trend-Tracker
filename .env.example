# Example environment variables for local development
# Copy this to `.env` and fill in real values (do NOT commit your `.env`).

# Public values (prefixed with EXPO_PUBLIC_ will be embedded for the client)
EXPO_PUBLIC_API_URL=http://192.168.0.158:8082
EXPO_PUBLIC_FEATURE_FLAG=false

# Private values (do NOT commit)
#OPENAI_API_KEY=
#ANTHROPIC_API_KEY=

# Optional: override Expo updates host/port for local testing
#EXPO_UPDATES_URL=http://192.168.0.158:8082
